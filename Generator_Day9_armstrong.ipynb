      "source": [
        "def armstr(lst):    \n",
        "    for num in lst:\n",
        "      order=len(str(num))\n",
        "      temp = num\n",
        "      s = 0\n",
        "      while temp > 0:\n",
        "        dig = temp % 10\n",
        "        s += dig ** order\n",
        "        temp=temp//10\n",
        "      if num == s:\n",
        "        yield num"
      ],
      "execution_count": 43,
      "outputs": []
    },
